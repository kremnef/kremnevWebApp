body {
    /*background: #32313B url("../../img/index-bg-gradient.png") top center no-repeat;*/
    background: #464D60 url("../../img/index-bg-gradient.png") top center no-repeat;
    margin-left: 50px;
    margin-right: 40px;
    font-family: Verdana, Arial, Tahoma, sans-serif;
}

.header {
    padding-top: 18px;
    margin: 0 auto;
    height: 108px;
    min-width: 800px;
    max-width: 1100px;
    text-align: left;
}

.logo {
    float: left;
    padding-right: 40px;
    margin-top: -1em;
    margin-left: -2em;
}

.header .instruments {
    padding-top: 25px;
}

.header .instruments .showHTML h1 {
    display: none;
}

.header .instruments a {
    color: #CCCCCC;
    text-decoration: none;
}

.header .contacts {
    float: left;
    font-size: 9px;
    text-transform: uppercase;
}

.header .instruments .showHTMLSimple {
    font-size: 8px;
    font-family: Verdana, Tahoma, Arial, sans-serif;
    text-transform: uppercase;
    float: right;
    padding-left: 35px;
}

.block-2 .showHTMLSimple {
    background: url("../../img/bg-search.png") left top no-repeat;
    padding: 5px 28px 5px 7px;
}

.block-2 .showHTMLSimple input {
    border: 0;
    background-color: transparent;
    width: 170px;
    color: #e7e7e7;
}

.map-block {
    clear: both;
    margin: -2em auto;
    min-width: 800px;
    max-width: 1100px;
    position: relative;
    text-align: left;
}

.map-block .left {
    float: left;
    position: absolute;
}

.map-block .left .logo-title {
    padding: 20px 0 70px 0;
    font-size: 13px;
    color: #858B99;
    text-transform: uppercase;
    width: 100px;
}

.map-block .verMenu {
    padding: 20px 0 70px 0;
    font-family: Arial, sans-serif;
    font-size: 11px;
    text-transform: uppercase;
    width: 100px;
}

.map-block .verMenu h2 {
    display: none;
}

.map-block .verMenu a {
    display: block;
    color: #FFCC00;
    text-decoration: underline;
    font-weight: bold;
    padding: 10px 0;
}

.map-block .verMenu .first a {
    color: #CCFF66;
    border-bottom: 1px solid #5E6270;
}

.map-block .showItem a {
    color: #CCFF66;
    border-bottom: 1px solid #5E6270;
}

.map-block .horMenu {
    padding: 20px 0 70px 0;
    font-family: Arial, sans-serif;
    font-size: 11px;
    text-transform: uppercase;
    width: 100px;
}

.map-block .horMenu a {
    display: block;
    color: #FFCC00;
    text-decoration: underline;
    font-weight: bold;
    padding: 10px 0;
}

.map-block .horMenu .first a {
    color: #CCFF66;
    border-bottom: 1px solid #5E6270;
}

.map-block #map #swf {
    background-color: transparent;
}

.map-block #map {
    margin-left: 40px;
    height: 330px;
    text-align: center;
    background-color: transparent;
}

.map-block #map h1 {
    display: none;
}

.map-block .float {
    position: absolute;
    padding-top: 140px;
    right: 0;
}

.map-block .float .verMenu {
    width: 100px;

}

.map-block .float .verMenu a {
    padding: 10px;
}

.block-6 .horMenu2Levels {
    height: 143px;
    background: url(../../img/bg-main-menu.png) repeat-x 0 40px;
    font-family: Arial, sans-serif;
    padding-top: 10px;
    padding-bottom: 20px;
    padding-left: 30px;
    margin: 0 auto;
    min-width: 800px;
    max-width: 1100px;
    text-align: left;
}

.block-6 .horMenu2Levels ul.level1 li.parent {
    float: left;
    width: 20%;
    text-align: left;
}

.block-6 .horMenu2Levels h3 {
    font-size: 14px;
    font-family: Arial, sans-serif;
    color: white;
    text-transform: uppercase;
    font-weight: normal;
    padding-bottom: 14px;
}

.block-6 .horMenu2Levels h3 a {
    color: white;
    text-decoration: none;

    font-size: 14px;
    font-family: Arial, sans-serif;
    color: white;
    text-transform: uppercase;
    font-weight: normal;

    text-shadow: rgba(0, 0, 0, 0.3) 0 -1px 0;
}

.block-6 .horMenu2Levels ul.level2 {
    margin-top: 10px;
}

.block-6 .horMenu2Levels ul.level2 li {
    margin-right: 60px;
    padding: 8px 0;
    border-top: 1px solid #353E53;
    color: #CCFF66;
}

.block-6 .horMenu2Levels ul.level2 li:first-child {
    border-top: 0;
}

.block-6 .horMenu2Levels ul.level2 li a:hover {
    color: #CCFF66;
    text-decoration: underline;
}

.block-6 .horMenu2Levels ul.level2 li a {
    display: block;
    font-size: 11px;
    /*color: #9AC050;*/
    color: #a0c753;
    text-decoration: none;

}

.content {
    margin: 0 auto;
    min-width: 800px;
    max-width: 1100px;
    text-align: left;
