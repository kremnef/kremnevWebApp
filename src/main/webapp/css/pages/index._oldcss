p, h1, h2, h3 {
    margin: 0;
    padding: 0;
}

h1, h2 {
    font-size: 24px;
    font-weight: normal;
    color: #000000;
}

ul, ol {
    margin: 0;
    padding: 0;
    list-style: none;
}

img {
    border: 0;
}

body {
    font-family: Verdana, Tahoma, sans-serif;
    font-size: 13px;
    text-align: center;
    background: #F7F7F7 url("../../img/bg-main.png") repeat-x scroll left top;
    /*background: #55586F url("../../img/bg-main.png") repeat-x scroll left top;*/
    margin: 0;
}

.clear {
    clear: both;
}

ul.files li {
    background: url(../../img/icons/any_icon.png) no-repeat center left;
    padding-left: 30px;
}

ul.files li.doc {
    background: url(../../img/icons/doc_icon.png) no-repeat center left;
}

.doc {
    padding-top: 10px;
}

.instruments .sitemap {
    float: right;
    padding: 0 10px;
}

.header .horMenu {
    clear: right;
    list-style: none outside none;
    margin: 0;
    min-width: 950px;
}

.header .horMenu li.first {
    padding: 10px 0 0;
}

.header .horMenu li {
    float: left;
    font-size: 14px;
    font-family: Arial, sans-serif;
    padding: 10px 0 0 8%;
    font-weight: normal;
    text-transform: uppercase;
}

.header .horMenu li a {
    color: white;
    white-space: nowrap;
    text-shadow: rgba(0, 0, 0, 0.3) 0 -1px 0;
}

.header .horMenu li.selected a {
    color: #FF6600;
}

#reception {
    margin: 0 0 0 100px;
}

.showLanguages {
    float: right;
    font-family: Verdana, sans-serif;
    font-size: 9px;
    text-transform: uppercase;
    color: #CCCCCC;
    padding: 5px 0 0 30px;
}

.showLanguages .selected {
    display: none;
}

