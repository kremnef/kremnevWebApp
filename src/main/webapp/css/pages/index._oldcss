.glyph, .glyphbox div span {
    font-family: 'WebSymbolsRegular';
}

.preview div, .glyphbox div {
    float: left;
}

.preview div {
    height: 3em;
    line-height: 3em;
    margin-right: 3em;
}

.glyphbox div {
    width: 9em;
    height: 8em;
    text-align: center;
}

.glyphbox, .preview {
    overflow: auto;
    width: 100%;
    margin-bottom: 2em;
}

.glyphbox div p {
    color: #666;
}

.glyphbox div p b {
    color: #333;
}

.new {
    color: #f00
}

